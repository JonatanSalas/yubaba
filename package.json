{
  "name": "yubaba",
  "private": true,
  "license": "MIT",
  "version": "0.1.9",
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "clean": "lerna clean --yes && rm -rf packages/core/dist/ && rm -rf packages/react/dist/",
    "build": "lerna run build",
    "lint": "eslint .",
    "test": "npm run lint && npm run unit && npm run flow",
    "unit": "npm run core:unit && npm run react:unit",
    "flow": "flow && flow stop",
    "core:dev": "lerna run start --scope *core",
    "core:tdd": "npm run core:unit -- -w",
    "core:unit": "mocha \"./packages/core/src/**/*.spec.js\" --opts ./packages/core/test/mocha.opts",
    "react:dev": "EXAMPLE=dark-side lerna run start --scope react*",
    "react:tdd": "npm run react:unit -- -w",
    "react:unit": "mocha \"./packages/react/src/**/*.spec.js\" --opts ./packages/react/test/mocha.opts",
    "codecov": "echo \"not implemented\"",
    "docs": "NODE_ENV=production bash ./scripts/buildDocs.sh",
    "publish": "npm run core:publish && npm run react:publish",
    "core:publish": "cd packages/core && npm publish",
    "react:publish": "cd packages/react && npm publish",
    "preversion": "npm test",
    "version": "version-changelog CHANGELOG.md && changelog-verify CHANGELOG.md && git add CHANGELOG.md && node ./scripts/deploy",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.6.1",
    "chai": "^4.0.2",
    "chai-as-promised": "^7.0.0",
    "changelog-verify": "^1.0.4",
    "eslint": "^3.1.1",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.0.0",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-react": "^7.1.0",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.51.0",
    "inject-loader": "^2.0.1",
    "jsdom": "^11.0.0",
    "jsdom-global": "^3.0.2",
    "lerna": "^2.0.0-rc.5",
    "lodash": "^4.17.4",
    "lolex": "^1.4.0",
    "mocha": "^3.4.2",
    "sinon": "^2.3.5",
    "sinon-chai": "^2.11.0",
    "version-changelog": "^2.1.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
